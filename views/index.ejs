
<div class="row p-5">
    <div class="col">

    </div>
    <% if(tweets) { %>
        <% tweets.forEach(tweet => { %>
            <div class="col card text-center">
                <div class="card-body p-4">
                    <h5 class="card-title"> <%= tweet.user.screen_name %> </h5>
                    <p class="card-text" id="tweetContent"><%= tweet.text %></p>
                </div>
            </div>
        <% }); %>
    <% } %>
    <div class="col">

    </div>
</div>